# Redis Configuration
REDIS_PASSWORD=<strong-redis-password-here>

# Backend Configuration
SECRET_KEY=<generate-with-openssl-rand-hex-32>
BACKEND_WORKERS=4

# Frontend Configuration
FRONTEND_PORT=80
