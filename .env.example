# ============================================
# B&R Capital Dashboard - Frontend Environment
# ============================================
# Copy this file to .env and fill in your values
# Vite requires VITE_ prefix for client-side env vars

# ===================
# API Configuration
# ===================

# Backend API URL (for production builds)
# In development, Vite proxy handles this automatically
VITE_API_URL=http://localhost:8000

# WebSocket URL (for production builds)
# In development, Vite proxy handles this automatically
VITE_WS_URL=ws://localhost:8000

# ===================
# External APIs
# ===================

# FRED API Key for interest rate data
# Get your free API key at: https://fred.stlouisfed.org/docs/api/api_key.html
VITE_FRED_API_KEY=your_fred_api_key_here

# ===================
# Feature Flags
# ===================

# Enable/disable experimental features
VITE_ENABLE_AI_INSIGHTS=true
VITE_ENABLE_REAL_TIME_UPDATES=true
VITE_ENABLE_EXPORT_FEATURES=true

# ===================
# Analytics (Optional)
# ===================

# Google Analytics tracking ID
# VITE_GA_TRACKING_ID=UA-XXXXXXXXX-X

# Sentry DSN for error tracking
# VITE_SENTRY_DSN=https://xxxxx@sentry.io/xxxxx

# ===================
# Development Settings
# ===================

# Enable verbose logging in development
VITE_DEBUG_MODE=false

# Mock API responses (useful for offline development)
VITE_USE_MOCK_DATA=false
